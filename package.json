    "react-app-rewired": "^1.5.0",
    "react-scripts": "1.1.1",
    "redux-logger": "^3.0.6"
  },
  "dependencies": {
    "antd": "^3.2.3",
    "antd-mobile": "^2.1.8",
    "axios": "^0.18.0",
